<p id="notice"><%= notice %></p>

<% @article.photos.each do |photo| %>
<%= cl_image_tag photo.key, height: 500, crop: :fill %>
<% end %>

<p>
  <strong>Title:</strong>
  <%= @article.title %>
</p>

<p>
  <strong>Body:</strong>
  <%= @article.body %>
</p>

<%= link_to 'Edit', edit_article_path(@article) %> |
<%= link_to 'Back', root_path %>
